{
  "name": "chatr-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "debug": "nodemon --inspect ./bin/www",
    "db:setup": "dropdb --if-exists --echo chatr_dev && createdb --echo chatr_dev && npm run db:migrate && npm run db:seed",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "ejs": "~2.5.5",
    "express": "^4.15.3",
    "morgan": "^1.9.1",
    "multer": "^1.3.0",
    "nodemon": "^1.18.9",
    "pg": "^7.6.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.41.0",
    "sequelize-cli": "^5.3.0",
    "serve-favicon": "^2.5.0"
  },
  "engines": {
    "node": "10.12.0"
  }
}
